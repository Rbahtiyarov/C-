//Объявление массива ввод/ввыдо значений
#include <iostream>
#include <vector>

using namespace std;

int main()
{
    int n;
    std::cin>>n;
    std::vector <int> a(n); // объявляем вектор a размера n
    
    for (int i = 0; i < n; i++) {   // i - начальная ячейка с какой начинаем заполнять значения, условие прекращения цикла пока значение не превысит количество введенных ячеек, переход к следующей ячейке 
        std::cin>>a[i];     // вносим значения в каждую ячейку вектора
        }
    for (int i = 0; i <= n - 1; i++) { // перебираем вектор от 0 до n-1 (так как нам нужно сравнивать количество ячеек, а не число)
        cout << a [i] << " "; // выводим результат через пробел
    } // вывод значений от нулевого до последнего
    
    for (int i = n - 1; i >= 0; i--) { // перебираем вектор от n-1 до 0 (так как нам нужно сравнивать количество ячеек, а не число)
        cout << a [i] << " "; // выводим результат через пробел
    } //вывод значений от последнего к нулевому
    return 0;
}
